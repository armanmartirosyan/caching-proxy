{
  "name": "cashing-proxy",
  "version": "1.0.0",
  "description": "A fast TypeScript/Node.js caching proxy using Redis to store and serve HTTP responses, reducing latency and backend load.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npx tsc && node ./dist/index.js",
    "dev": "npx nodemon",
    "prepare": "npx husky",
    "lint": "npx eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npx eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armanmartirosyan/caching-proxy.git"
  },
  "keywords": [
    "caching-proxy",
    "typescript",
    "redis"
  ],
  "author": "Arman Martirosyan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/armanmartirosyan/caching-proxy/issues"
  },
  "homepage": "https://github.com/armanmartirosyan/caching-proxy#readme",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "husky": "^9.1.7",
    "redis": "^5.5.6"
  }
}
